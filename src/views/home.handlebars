<div class="container">
  <h1>Lista de Productos</h1>
  <div class="products">
    {{#each products}}
      <div class="product-card">
        <h2>{{this.name}}</h2>
        <p>{{this.description}}</p>
        <p>Precio: ${{this.price}}</p>
        <p>Stock: {{this.stock}}</p>
      </div>
    {{/each}}
  </div>
</div>
